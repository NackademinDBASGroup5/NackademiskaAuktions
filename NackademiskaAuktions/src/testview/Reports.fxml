<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testcontrollers.ReportCon">
   <children>
      <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
         <children>
            <VBox minWidth="150.0" prefHeight="200.0" prefWidth="150.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Välj rapport:">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Text>
                  <Button fx:id="customerReportButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Kunder som handlat" wrapText="true">
                     <tooltip>
                        <Tooltip text="Lista alla kunder som har handlat och hur mycket varje kund har handlat för" />
                     </tooltip>
                  </Button>
                  <Button fx:id="provisionPerMonthButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Provision per månad">
                     <tooltip>
                        <Tooltip text="Lista provisionen för alla månader som har auktioner" />
                     </tooltip>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</Pane>
